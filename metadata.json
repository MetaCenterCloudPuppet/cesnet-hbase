{
  "name": "cesnet-hbase",
  "version": "1.1.0",
  "author": "Frantisek Dvorak <valtri@civ.zcu.cz>",
  "summary": "HBase Cluster",
  "license": "MIT",
  "source": "https://github.com/MetaCenterCloudPuppet/cesnet-hbase",
  "project_page": "https://github.com/MetaCenterCloudPuppet/cesnet-hbase",
  "issues_url": "https://github.com/MetaCenterCloudPuppet/cesnet-hbase/issues",
  "tags": [
    "hadoop",
    "hbase"
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": ["6", "7"]
    },
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": ["7", "8", "9"]
    },
    {
      "operatingsystem": "Fedora"
    },
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": ["6", "7"]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": ["6", "7"]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": ["14.04", "16.04", "18.04"]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 3.4.0"
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 1.0.0 <5.0.0"
    },
    {
      "name": "cesnet/hadoop",
      "version_requirement": ">= 0.9.4 <4.0.0"
    },
    {
      "name": "cesnet/hadoop_lib",
      "version_requirement": "<1.0.0"
    }
  ]
}
